{
  "name": "angular-chaos-monkey",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "version": "0.0.0",
  "license": "MIT",
  "ngPackage": {
    "lib": {
      "entryFile": "src/public_api.ts"
    },
    "dest": "dist",
    "deleteDestPath": true
  },
  "scripts": {
    "start": "",
    "build": "ng-packagr -p package.json",
    "format": "clang-format -i --glob=src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": {
    "name": "Michael Ware",
    "email": "mikrusfarr@gmail.com"
  },
  "homepage": "https://github.com/Zaljin/angular-chaos-monkey#readme",
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@types/jest": "^23.3.1",
    "clang-format": "^1.2.4",
    "jest": "^23.5.0",
    "jest-marbles": "^2.0.0",
    "jest-preset-angular": "^6.0.0",
    "ng-packagr": "^4.1.0",
    "rxjs": "^6.2.0",
    "ts-node": "^7.0.1",
    "tsickle": "^0.27.3",
    "tslib": "^1.9.0",
    "typescript": "~2.7.0",
    "zone.js": "^0.8.26"
  }
}
